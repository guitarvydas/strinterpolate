strint {
  main = statement
  statement =
    | "print" space+ "\"" str1ng "\""
  str1ng =
    | basicString str1ng -- basicPair
    | basicString -- basic
    | interpolation str1ng -- interpolationPair
    | interpolation -- finalInterpolation
  basicString = notSpecial+
  interpolation = "◎" "⦅" str1ng "⦆"
  notSpecial = ~"◎" ~"⦅" ~"⦆" ~"\"" any
}

