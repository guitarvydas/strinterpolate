strint {
  main = statement+
  statement =
    | "print" space+ "\"" string "\"" spaces
  string =
    | basicString string -- basicPair
    | basicString -- basic
    | interpolation string -- interpolationPair
    | interpolation -- finalInterpolation
  basicString = notSpecial+
  interpolation = "$" "{" string "}"
  notSpecial = ~("$" "{") ~"}" ~"\"" any
}

